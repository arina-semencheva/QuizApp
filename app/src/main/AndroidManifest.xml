<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edmt.dev.androidonlinequizapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_public_black_24dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_public_black_24dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Home" />
        <activity android:name=".Start" />
        <activity android:name=".Playing" />
        <activity
            android:name=".Done"
            android:label="@string/title_activity_done"
            android:theme="@style/AppTheme" />
        <activity android:name=".Soon"></activity>
    </application>

</manifest>